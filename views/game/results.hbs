<div class="container">

  <h1 class="title">Finish!</h1>
  <h3 class="question">Right answers:
    {{total_right_answers}}
    /
    {{num_questions}}</h3>
  <h3 class="profile-points">
    {{userData.username}}
    {{total_points}}
    POINTS WON!</h3>

  <div class="card-container">
    <div class="card-select">
      {{#each answersArrObject}}
        {{#if this.answersArray}}
          <h3 class="qCorrect">Q{{index}} Correct!</h3>
        {{else}}
          <h3 class="qWrong">Q{{index}} Wrong!</h3>
        {{/if}}
      {{/each}}
    </div>
  </div>

  <h4>Authors: Elisa B. & Dani G.</h4>
  <h4>TA: Ale Baus√°</h4>

  <footer class="footer">
    <ul>
      <a class="homelink noformcontainer" href="/profile"><img
          width="50"
          height="50"
          src="/navbar/profile.png"
        /></a>
    </ul>
  </footer>

</div>