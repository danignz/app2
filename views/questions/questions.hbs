<div class="container">
  <h1 class="title">List of Questions</h1>

  <div class="card-container">
    {{#if questions}}
      {{#each questions}}
        <div class="card">
          <h4> Â·{{this.question}}</h4>
          <h6> - {{this.category}} - </h6>
          <h3><a class="details" href="/questions/{{this._id}}">See details</a></h3>
        </div>
      {{/each}}
    {{else}}
      <h3>Not questions avariable</h3>
    {{/if}}
  </div>

  <footer class="footer">
    <ul>
      <li><button onclick="history.back()"><img
            width="45"
            height="45"
            src="/navbar/back.png"
          /></button></li>
      <li>
        <form action="/auth/logout" method="POST"><button type="submit"><img
              width="45"
              height="45"
              src="/navbar/logout.png"
            /></button></form>
      </li>
    </ul>
  </footer>

</div>