<h1 class="title">List of Quizzes</h1>

<div>
  {{#if quizzes}}
    {{#each quizzes}}
      <h4><a href="/quizzes/{{this._id}}">{{this.title}}</a></h4>
      <h6> - {{this.category}} - </h6>
      <h6> - {{this.difficulty}} - </h6>
      <h3 class="details" >List of Questions:</h3>
      {{#each this.question}}
        <h6><a href="/questions/{{this._id}}">ID: {{this._id}}</a></h6>
      {{/each}}
    {{/each}}
  {{else}}
    <h3>Not quizzes avariable</h3>
  {{/if}}
</div>

<a class="homelink" href="/">Back Home</a>